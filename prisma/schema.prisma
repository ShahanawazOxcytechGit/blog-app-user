model Employee {
  id             Int    @id @default(autoincrement())
  email          String @unique
  username       String
  password       String
  hashedPassword String
  access         String
  action         String
}

model TutorialSubtopic {
  id       Int     @id @default(autoincrement())
  title    String
  metadata String?
  content  String
}

model Blog {
  id       Int     @id @default(autoincrement())
  title    String  @db.VarChar(255)
  metaData String?
  image    String
  content  String?
  slug     String
}
