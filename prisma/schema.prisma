generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgres://default:wtAkbT4m0ydj@ep-ancient-poetry-a1ddrdv4.ap-southeast-1.aws.neon.tech:5432/verceldb?sslmode=require"
